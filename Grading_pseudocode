START
  PROMPT user to enter student's score
  READ score

  IF score >= 70 AND score <= 100
      grade = 'A'
  ELSE IF score >= 60 AND score <= 69
      grade = 'B'
  ELSE IF score >= 50 AND score <= 59
      grade = 'C'
  ELSE IF score >= 45 AND score <= 49
      grade = 'D'
  ELSE IF score >= 40 AND score <= 44
      grade = 'E'
  ELSE IF score >= 0 AND score < 40
      grade = 'F'
  ELSE
      PRINT "Invalid score"
      STOP

  PRINT "Grade:", grade
END
